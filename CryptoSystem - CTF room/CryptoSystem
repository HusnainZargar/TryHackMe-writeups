from Crypto.Util.number import *
from sympy import isprime, nextprime
import math

c = 

n = 

e = 0x10001

def factor_n(n):
    approx_p = int(math.isqrt(n))
    if approx_p % 2 == 0:
        approx_p -= 1

    for i in range(10000):  
        p = approx_p - i
        if isprime(p):
            q = nextprime(p)
            if p * q == n:
                return p, q
    return None, None

p, q = factor_n(n)
if p is None:
    print("Failed to factor n")
    exit()

phi = (p - 1) * (q - 1)
d = inverse(e, phi)

m = pow(c, d, n)
plaintext = long_to_bytes(m)
print(f"[+] Recovered FLAG: {plaintext.decode()}")
